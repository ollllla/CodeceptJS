{
    "name": "symfony/symfony-demo",
    "license": "MIT",
    "type": "project",
    "description": "Symfony Demo Application",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3|^8.0",
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^1.6.10|^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "erusev/parsedown": "^1.6",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^5.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/debug-bundle": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/intl": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/polyfill-php72": "^1.8",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/web-server-bundle": "^4.0",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "*",
        "tgalopin/html-sanitizer-bundle": "^1.1",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.6"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0",
        "codeception/module-doctrine2": "^1.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-sequence": "^1.0",
        "codeception/module-symfony": "^1.0",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "dama/doctrine-test-bundle": "^5.0|^6.0|^7.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "testomatio/list-tests": ">=0.1.4",
        "testomatio/reporter": "^0.2.0",
        "vlucas/phpdotenv": ">2.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
